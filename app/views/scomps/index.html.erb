<div class="container-fluid">
      <div class="row">
        <div class="col-md-2 sidebar">
          <div class="row placeholders">
          <img src="http://www.ccareynkf.com/FileStore/IMG/Agt_59B4875E12004ED397A4A176FE35AD46.jpg" width="130" height="130" class="img-circle"/>
            <h4>Jason Forcier</h4>
              <span class="text-muted">Cornish & Carey</span>
            </div>
          <%= render 'layouts/navbarside' %>
          </div><!--column-->
        </div><!--row-->
      </div><!--container-->
<!-- Sidebar Nav -->
<!-- Dashboard Header -->
<div class="row">
        <div class="col-md-10 col-md-offset-2">
          <h2 class="page-header"><p><i class="fa fa-building-o fa-2x fa-fw"></i>Sale Comps</p>
        </div><!--column-->
      </div><!--row-->
          <div class="row">
        <div class="col-md-10 col-md-offset-2">

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Building Photo</th>
      <th>City</th>
      <th>Address</th>
      <th>Size</th>
      <th>Sale Price</th>
      <th>Cap Rate</th>
      <th>Closing Date</th>
      <th>Buyer</th>
      <th>Seller</th>
      <th>Building Type</th>
      <th>Building Class</th>
      <th>Sale Type</th>
    </tr>
  </thead>

  <tbody>
    <% @scomps.each do |scomp| %>
      <tr>
        <td><%= image_tag scomp.image.url(:thumb) %></td>
        <td><small><%= scomp.city %></small></td>
        <td><small><%= scomp.address %></small></td>
        <td><small><%= scomp.size %></small></td>
        <td><small><%= scomp.sale_price %></small></td>
        <td><small><%= scomp.cap_rate %></small></td>
        <td><small><%= scomp.closing_date %></small></td>
        <td><small><%= scomp.buyer %></small></td>
        <td><small><%= scomp.seller %></small></td>
        <td><small><%= scomp.building_type %></small></td>
        <td><small><%= scomp.building_class %></small></td>
        <td><small><%= scomp.sale_type %></small></td>
        <td><%= link_to scomp do %>
          <i class="fa fa-building-o fa-lg fa-fw"></i><small>View</small>
        <% end %></td>
        <td><%= link_to edit_scomp_path(scomp) do %>
          <i class="fa fa-pencil-square-o fa-lg fa-fw"></i><small>Edit</small>
          <% end %>
        </td>
        <td><%= link_to scomp, method: :delete, data: { confirm: 'Are you sure?' } do %>
           <i class="fa fa-trash-o fa-lg fa-fw"></i><small>Delete</small>
        <% end %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scomp', new_scomp_path %>
