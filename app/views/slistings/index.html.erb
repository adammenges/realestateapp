<div class="container-fluid">
      <div class="row">
        <div class="col-md-2 sidebar">
          <div class="row placeholders">
          <img src="http://www.ccareynkf.com/FileStore/IMG/Agt_59B4875E12004ED397A4A176FE35AD46.jpg" width="130" height="130" class="img-circle"/>
            <h4>Jason Forcier</h4>
              <span class="text-muted">Cornish & Carey</span>
            </div>
          <%= render 'layouts/navbarside' %>
          </div><!--column-->
        </div><!--row-->
      </div><!--container-->
<!-- Sidebar Nav -->
<!-- Dashboard Header -->
<div class="row">
        <div class="col-md-10 col-md-offset-2">
          <h2 class="page-header"><p><i class="fa fa-building-o fa-2x fa-fw"></i>Available - For Sale</p>
        </div><!--column-->
      </div><!--row-->

          <div class="row">
        <div class="col-md-10 col-md-offset-2">

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Image</th>
      <th>City</th>
      <th>Address</th>
      <th>Size</th>
      <th>Price</th>
      <th>Building Type</th>
      <th>Building Class</th>
      <th>Sale Type</th>
      <th>Floors</th>
      <th>Parking Ratio</th>
      <th>Brokerage</th>
      <th>Notes</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @slistings.each do |slisting| %>
      <tr>
        <td><%= image_tag slisting.image.url(:thumb) %></td>
        <td><small><%= slisting.city %></small></td>
        <td><small><%= slisting.address %></small></td>
        <td><small><%= slisting.size %></small></td>
        <td><small><%= slisting.price %></small></td>
        <td><small><%= slisting.building_type %></small></td>
        <td><small><%= slisting.building_class %></small></td>
        <td><small><%= slisting.sale_type %></small></td>
        <td><small><%= slisting.floors %></small></td>
        <td><small><%= slisting.parking_string %></small></td>
        <td><small><%= slisting.brokerage %></small></td>
        <td><small><%= slisting.notes %></small></td>
        <td><%= link_to slisting do %>
        <i class="fa fa-building-o fa-lg fa-fw"></i><small>View</small>
        <% end %></td>
        <td><%= link_to edit_slisting_path(slisting) do %>
          <i class="fa fa-pencil-square-o fa-lg fa-fw"></i><small>Edit</small>
          <% end %>
        </td>
        <td><%= link_to slisting, method: :delete, data: { confirm: 'Are you sure?' } do %>
          <i class="fa fa-trash-o fa-lg fa-fw"></i><small>Delete</small>
        <% end %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sale Listing', new_slisting_path %>
