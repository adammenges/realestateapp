<%= link_to 'Back', slistings_path %>


<div class="row">
  <div class="col-md-offset-2 col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading center">
        <%= image_tag @slisting.image.url(:medium) %>
      </div>
      <div class="panel-body">


<p>
  <strong>City:</strong>
  <%= @slisting.city %>
</p>

<p>
  <strong>Address:</strong>
  <%= @slisting.address %>
</p>

<p>
  <strong>Size:</strong>
  <%= @slisting.size %>
</p>

<p>
  <strong>Price:</strong>
  <%= @slisting.price %>
</p>

<p>
  <strong>Building type:</strong>
  <%= @slisting.building_type %>
</p>

<p>
  <strong>Building class:</strong>
  <%= @slisting.building_class %>
</p>

<p>
  <strong>Sale type:</strong>
  <%= @slisting.sale_type %>
</p>

<p>
  <strong>Floors:</strong>
  <%= @slisting.floors %>
</p>

<p>
  <strong>Parking string:</strong>
  <%= @slisting.parking_string %>
</p>

<p>
  <strong>Brokerage:</strong>
  <%= @slisting.brokerage %>
</p>

<p>
  <strong>Notes:</strong>
  <%= @slisting.notes %>
</p>

<% if @slisting.user == current_user %>
<%= link_to 'Edit', edit_slisting_path(@slisting) %> |
<% end %>

       </div>
      </div>
    </div>
  </div>
