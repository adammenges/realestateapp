<div class="container-fluid">
      <div class="row">
        <div class="col-md-2 sidebar">
          <div class="row placeholders">
          <img src="http://www.ccareynkf.com/FileStore/IMG/Agt_59B4875E12004ED397A4A176FE35AD46.jpg" width="130" height="130" class="img-circle"/>
            <h4>Jason Forcier</h4>
              <span class="text-muted">Cornish & Carey</span>
            </div>
          <%= render 'layouts/navbarside' %>
          </div><!--column-->
        </div><!--row-->
      </div><!--container-->
<!-- Sidebar Nav -->
<!-- Dashboard Header -->
<div class="row">
        <div class="col-md-10 col-md-offset-2">
          <h2 class="page-header"><p><i class="fa fa-building-o fa-2x fa-fw"></i>Lease Comps</p>
        </div><!--column-->
      </div><!--row-->
          <div class="row">
        <div class="col-md-10 col-md-offset-2">
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Building Photo</th>
      <th>City</th>
      <th>Address</th>
      <th>Suite</th>
      <th>Size</th>
      <th>Effective Rate</th>
      <th>Lease Start</th>
      <th>Lease Expiration</th>
      <th>Tenant</th>
      <th>Landlord</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @comps.each do |comp| %>
      <tr>
        <td><%= image_tag comp.image.url(:thumb) %></td>
        <td><small><%= comp.city %></small></td>
        <td><small><%= comp.address %></small></td>
        <td><small><%= comp.suite %></small></td>
        <td><small><%= comp.size %></small></td>
        <td><small><%= comp.effective_rate %></small></td>
        <td><small><%= comp.lease_start %></small></td>
        <td><small><%= comp.lease_expiration %></small></td>
        <td><small><%= comp.tenant %></small></td>
        <td><small><%= comp.landlord %></small></td>
        <td><%= link_to detail_comp_path do %>
        <i class="fa fa-building-o fa-lg fa-fw"></i><small>View</small>
        <% end %></td>
        <td><%= link_to edit_comp_path(comp) do %>
          <i class="fa fa-pencil-square-o fa-lg fa-fw"></i><small>Edit</small>
          <% end %>
        </td>
        <td><%= link_to comp, method: :delete, data: { confirm: 'Are you sure?' } do %>
        <i class="fa fa-trash-o fa-lg fa-fw"></i><small>Delete</small>
        <% end %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comp', new_comp_path %>
