<!--forleasetabl-->
<div class="row top-buffer">
   <div class="col-xs-12 col-md-10 col-md-offset-2">

    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Search Buildings For Lease</h3>
      </div>
    <div class="panel-body">
      <div class="row">
        <div class="col-xs-12 col-md-12">

      <%= search_form_for @search do |f| %>

        <!-- City Search -->
        <div class="row">
          <div class="field-group"> 
        <div class="col-xs-6 col-md-2">
            <%= f.label :city_cont, "City" %>
        </div><!--column-->
          <div class="col-xs-6 col-md-10">
            <%= f.text_field :city_cont %>
        </div><!--column-->
        </div><!--fieldgroup-->
          </div><!--row-->

         <!-- Building Type -->
         <div class="row top-buffer">
          <div class="field-group">
            <div class="col-xs-6 col-md-2">
            <%= f.label :building_type_cont, "Building Type" %>
        </div><!--column-->
        <div class="col-xs-6 col-md-10">
            <%= f.text_field :building_type_cont %>
        </div><!--column-->
      </div><!--fieldgroup-->
          </div><!--row-->


          <!-- Building Class -->
          <div class="row top-buffer">
          <div class="field-group">
            <div class="col-xs-6 col-md-2">
            <%= f.label :building_class_cont, "Building Class" %>
        </div><!--column-->
        <div class="col-xs-6 col-md-10">
            <%= f.text_field :building_class_cont %>
        </div><!--column-->
        </div><!--fieldgroup-->
        </div><!--row-->

        <!-- Size (sf) -->
        <div class="row top-buffer">
          <div class="field-group">
            <div class="col-xs-6 col-md-2">
            <%= f.label :size_gteq, "Minimum Size (sf)" %>
        </div><!--column-->
        <div class="col-xs-6 col-md-2">
            <%= f.text_field :size_gteq %>
          </div><!--column-->
          <div class="col-xs-6 col-md-2">
            <%= f.label :size_lteq, "Maximum Size (sf)" %>
            </div><!--column-->
          <div class="col-xs-6 col-md-2">
            <%= f.text_field :size_lteq %>
          </div><!--column-->
        </div><!--fieldgroup-->
      </div><!--row-->

          <!-- Asking Rate -->
          <div class="row top-buffer">
           <div class="field-group">
            <div class="col-xs-6 col-md-2">
              <%= f.label :asking_rate_gteq, "Minimum Rate ($)" %>
          </div><!--column-->
          <div class="col-xs-6 col-md-2">
              <%= f.text_field :asking_rate_gteq %>
          </div><!--column-->
          <div class="col-xs-6 col-md-2">
              <%= f.label :asking_rate_lteq, "Maximum Rate ($)" %>
          </div><!--column-->
          <div class="col-xs-6 col-md-2">
              <%= f.text_field :asking_rate_lteq %>
            </div><!--column-->
        </div><!--fieldgroup-->
      </div><!--row-->

        <!-- Search Button -->
        <div class="row">
          <div class="col-xs-6 col-md-6">
          <div class="actions">
            <%= f.submit "Search" %></div>
        <% end %>
      </div><!--column-->
      </div><!--row-->

                 </div><!--panelbdy-->
                </div><!--paneldefault-->
              </div><!--column-->
       </div><!--row-->
