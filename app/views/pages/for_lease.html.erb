<!-- Sidebar Nav -->
<!-- Dashboard Header -->
<div class="row">
        <div class="col-xs-12 col-md-10 col-md-offset-2">
          <h2 class="page-header"><p><i class="fa fa-search fa-2x"></i> For Lease</p></h2>
        </div>
      </div>
<!--navtab-->
<div class="row">
  <div class="col-xs-12 col-md-10 col-md-offset-2">
    <ul class="nav nav-tabs">
    <li class="active" ><%= link_to "For Lease", "for_lease"%></li>
    <li><%= link_to "Lease Comps", "lease_comps"%></li>
    <li><%= link_to "For Sale", "for_sale"%></li>
    <li><%= link_to "Sale Comps", "sale_comps"%></li>
    <li><%= link_to "Property Directory", "#"%></li>
  </ul> <!--navtabs-->
  </div> <!--column-->
